@import "/vendor/css/main.wxss";

@charset "UTF-8";
.main-inner {
  -webkit-overflow-scrolling: touch;
  background: #f2f2f2;
  font-family: 'microsoft yahei';
}
.logo {
  width: 184rpx;
  height: 58rpx;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: cover;
}
.loginMain {
  padding: 58rpx 66rpx 0;
}
.loginMain ._header {
  font-size: 40rpx;
  line-height: 40rpx;
}
.loginMain ._h1 {
  margin-top: 20rpx;
  font-size: 40rpx;
  color: #222222;
  line-height: 40rpx;
}
.loginMain .formContent {
  margin-top: 76rpx;
  font-size: 0;
}
.loginMain .formContent .labelBox {
  margin-bottom: 36rpx;
}
.loginMain .formContent .labelBox.sCode {
  margin-bottom: 80rpx;
}
.loginMain .formContent .labelName {
  display: inline-block;
  vertical-align: middle;
  width: 180rpx;
  font-size: 34rpx;
  color: #909090;
  line-height: 34rpx;
}
.loginMain .formContent .labelText {
  display: inline-block;
  vertical-align: middle;
  box-sizing: border-box;
  border: 1rpx solid #CCCCCC;
  border-radius: 10rpx;
  width: 438rpx;
  height: 94rpx;
  font-size: 34rpx;
  line-height: normal;
  padding: 0 12rpx 0 28rpx;
  outline-style: none;
  text-shadow: none;
  -webkit-appearance: none;
  -webkit-user-select: text;
  box-shadow: none;
}
.loginMain .formContent .labelText::-webkit-input-placeholder {
  color: #aaa;
  line-height: normal;
}
.loginMain .formContent .labelText:-moz-placeholder {
  line-height: normal;
}
.loginMain .formContent .labelText::-moz-placeholder {
  color: #aaa;
  line-height: normal;
}
.loginMain .formContent .labelText:-ms-input-placeholder {
  color: #aaa;
  line-height: normal;
}
.loginMain .formContent .sendCodeBox {
  display: inline-block;
  vertical-align: middle;
  box-sizing: border-box;
  border: 1rpx solid #CCCCCC;
  border-radius: 10rpx;
  width: 438rpx;
  height: 94rpx;
  font-size: 34rpx;
  line-height: normal;
  padding: 0 12rpx 0 28rpx;
}
.loginMain .formContent .sendCodeBox .labelText {
  border: none;
  height: 100%;
  width: 178rpx;
}
.loginMain .formContent .sendCodeBox .labelText.getCode {
  border: none;
  height: 100%;
  padding: 0;
}
.loginMain .formContent .sendCodeBox .sendCode {
  display: inline-block;
  vertical-align: bottom;
  margin-bottom: 26rpx;
  font-size: 34rpx;
  color: #2FC5BD;
  line-height: 34rpx;
}
.loginMain .formContent .sendCodeBox .sendCode:before {
  content: '';
  display: inline-block;
  vertical-align: middle;
  width: 2rpx;
  height: 32rpx;
  background-color: #ccc;
  margin-right: 28rpx;
  margin-top: -4rpx;
}
.loginMain .formContent .sendCodeBox .sendCode ._i {
  display: inline-block;
}
.loginMain .formContent .sendCodeBox .sendCode.onSend:before {
  content: '';
  display: inline-block;
  vertical-align: middle;
  width: 2rpx;
  height: 32rpx;
  background-color: #ccc;
  margin-right: 18rpx;
  margin-top: -4rpx;
}
.sureBtn {
  text-align: center;
  width: 540rpx;
  height: 110rpx;
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: 1.06rpx;
  line-height: 110rpx;
  background-color: #00B9AD;
  border-radius: 50rpx;
  margin: 0 auto;
}
.report-inner {
  background: #fff;
  height: 100vh;
  overflow-x: hidden;
  overflow-y: auto;
}
.report-inner .healthCooperationHeader {
  height: 130rpx;
  line-height: 130rpx;
  background: #fff;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
  font-size: 40rpx;
  font-weight: 600;
  flex-direction: row;
  color: #222;
}
.report-inner .healthCooperationHeader ._span {
  margin-left: 10rpx;
}
.report-inner .healthCooperationImages {
  height: 126rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  background: #fff;
  border-top: 16rpx solid #f2f2f2;
  border-bottom: 1rpx solid #ededed;
}
.report-inner .healthCooperationImages ._span {
  display: inline-block;
}
.report-inner .healthCooperationImages .imageNumber {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  height: 40rpx;
  line-height: 40rpx;
  color: #222;
  font-size: 36rpx;
}
.report-inner .healthCooperationImages .imageNumber .icon {
  display: inline-block;
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: contain;
  height: 40rpx;
  width: 40rpx;
  margin-right: 16rpx;
}
.report-inner .healthCooperationImages .check-btn {
  background: #00B9AD;
  border-radius: 8rpx;
  color: #fff;
  font-size: 30rpx;
  width: 116rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
}
.report-inner .healthCooperationReportList {
  min-height: 1000rpx;
  background: #fff;
  padding-bottom: 100rpx;
  margin: 0 auto;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem {
  border-bottom: 1rpx solid #ededed;
  padding: 0 7.7% 0 4.8%;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .title {
  font-size: 40rpx;
  color: #222;
  height: 132rpx;
  line-height: 132rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .subTitle {
  position: relative;
  height: 36rpx;
  font-size: 36rpx;
  color: #222;
  line-height: 36rpx;
  margin-bottom: 40rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .subTitle ._span {
  display: inline-block;
  margin-left: 20rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .subTitle ._i {
  position: absolute;
  left: 0;
  background: #00B9AD;
  border-radius: 4rpx;
  width: 8rpx;
  height: 28rpx;
  top: 50%;
  margin-top: -14rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe {
  font-size: 32rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe:nth-of-type(n) {
  margin-bottom: 50rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe:nth-of-type(1) {
  margin-bottom: 40rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe .describeTitle {
  color: #222;
  margin-left: 8rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe .describeTitle ._i {
  width: 10rpx;
  height: 10rpx;
  display: inline-block;
  border-radius: 50%;
  background: #00B9AD;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe .describeTitle ._span {
  margin-left: 10rpx;
}
.report-inner .healthCooperationReportList .healthCooperationReportItem .reportDescribe .describeContent {
  color: #333;
  margin-left: 28rpx;
  line-height: 42rpx;
  margin-top: 19rpx;
}
.report-inner .footer-btn {
  position: fixed;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  align-items: center;
  bottom: 34rpx;
  left: 0;
  right: 0;
  width: 520rpx;
  height: 96rpx;
  background: #00B9AD;
  margin: 0 auto;
  border-radius: 200rpx;
  box-shadow: 0 4rpx 8rpx 0 rgba(198, 230, 228, 0.5), 0 16rpx 34rpx 0 rgba(181, 230, 227, 0.87);
  color: #fff;
  font-size: 36rpx;
  line-height: 96rpx;
}
.report-inner .footer-btn .icon {
  background: url("data:image/png;base64,") no-repeat center center;
  background-size: contain;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  margin-right: 10rpx;
}
.report-attachment-inner {
  background: #fff;
  padding: 0 8%;
}
.report-attachment-inner .title {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  padding-top: 48rpx;
  height: 34rpx;
  line-height: 34rpx;
}
.report-attachment-inner .title ._i {
  width: 10rpx;
  height: 10rpx;
  background: #2FC5BD;
  border-radius: 50%;
  display: inline-block;
  margin-right: 10rpx;
}
.report-attachment-inner .title .subTitle {
  display: inline-block;
  font-size: 32rpx;
  color: #222;
  margin-right: 8rpx;
}
.report-attachment-inner .title .des {
  font-size: 24rpx;
  color: #909090;
  font-weight: 200;
}
.report-attachment-inner .report-attachment-list {
  width: 100%;
  height: auto;
  min-height: 100rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.report-attachment-inner .report-attachment-list .report-attachment-item {
  width: 140rpx;
  height: 140rpx;
  border: 1rpx solid #CCCCCC;
  border-radius: 4rpx;
  overflow: hidden;
  background: #cecece;
  margin: 0 18rpx 18rpx 0;
}
.report-attachment-inner .report-attachment-list .report-attachment-item:nth-of-type(4n) {
  margin-right: 0;
}
.report-attachment-inner .report-attachment-list .report-attachment-item:nth-of-type(1),
.report-attachment-inner .report-attachment-list .report-attachment-item:nth-of-type(2),
.report-attachment-inner .report-attachment-list .report-attachment-item:nth-of-type(3),
.report-attachment-inner .report-attachment-list .report-attachment-item:nth-of-type(4) {
  margin-top: 18rpx;
}
.report-attachment-inner .report-attachment-list .report-attachment-item ._img {
  width: 100%;
  height: 100%;
}
.report-attachment-inner .returnBack {
  width: 108rpx;
  height: 54rpx;
  line-height: 54rpx;
  background: #00B9AD;
  border-radius: 8rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  position: absolute;
  right: 4%;
  top: 24rpx;
}
.patient-inner {
  background: #f2f2f2;
  height: 100vh;
  overflow-x: hidden;
  overflow-y: auto;
}
.patient-inner .patient-card {
  width: 622rpx;
  height: 388rpx;
  background-color: #fff;
  border-radius: 16rpx;
  margin: 76rpx auto;
  position: relative;
}
.patient-inner .patient-card .bg {
  position: absolute;
  width: 158rpx;
  height: 170rpx;
  right: 32rpx;
  bottom: 22rpx;
}
.patient-inner .patient-card .title {
  padding: 60rpx 0 32rpx 57rpx;
  font-size: 34rpx;
  color: rgba(34, 34, 34, 0.7);
}
.patient-inner .patient-card .patient-name {
  font-size: 50rpx;
  color: #222;
  padding-left: 57rpx;
}