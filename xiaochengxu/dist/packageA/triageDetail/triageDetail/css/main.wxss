@import "/vendor/css/main.wxss";

.he-videoUpHide {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  background-color: #ffffff;
  box-sizing: border-box;
}
.he-videoUpHide .he-videoUpLoadBox {
  height: 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain {
  min-height: 100%;
  height: auto;
  box-sizing: border-box;
  position: relative;
  padding-bottom: 117rpx;
  background-color: #fff;
  z-index: 5;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .al-uploadNumLimit {
  font-size: 32rpx;
  padding: 30rpx 60rpx 0 48rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .al-uploadNumLimit ._span {
  display: inline-block;
  position: relative;
  padding: 10rpx 30rpx 10rpx 65rpx;
  background-color: #fa787a2b;
  border-radius: 50rpx;
  width: 100%;
  box-sizing: border-box;
  color: #444444;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .al-uploadNumLimit ._span::before {
  position: absolute;
  content: "";
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background: url("https://m.allinmed.cn/static/image/img00/doctorHome/upLoadTip.png") no-repeat center;
  background-size: 28rpx 28rpx;
  top: 50%;
  margin-top: -14rpx;
  left: 20rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadTitle {
  padding: 48rpx 40rpx 10rpx;
  font-size: 34rpx;
  line-height: 1.5;
  color: #222222;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadTitle.upLoadPicHasTip {
  padding: 30rpx 40rpx 10rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles {
  padding: 0 40rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles:after {
  content: '';
  display: table;
  clear: both;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles.he-videoImageBox {
  padding-left: 32rpx;
  padding-right: 40rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles.he-videoImageBox .tc-upLoadInput {
  cursor: pointer;
  position: absolute;
  right: 0;
  opacity: 0;
  outline: none;
  width: 100%;
  height: 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-loadVideoSuccess {
  width: 152rpx;
  height: 152rpx;
  text-align: center;
  padding: 8rpx;
  opacity: 0.7;
  background: #000000;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-loadVideoSuccess .he-loadVideoSuccessBox {
  display: block;
  padding-top: 40rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-loadVideoSuccess .he-loadVideoSuccessBox .he-loadSuccessTip {
  display: block;
  margin: 0 auto;
  width: 36rpx;
  height: 36rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/upload_success.png");
  background-size: 100% 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-loadVideoSuccess .he-loadVideoSuccessBox .he-loadSuccessPercent {
  display: block;
  text-align: center;
  color: #ffffff;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-loadVideoSuccess .he-loadVideoSuccessBox .he-loadSuccessText {
  font-size: 24rpx;
  display: block;
  color: #ffffff;
  padding-top: 10rpx;
  text-align: center;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddBtn.he-loadSuccessTextBox {
  width: 152rpx;
  height: auto;
  text-align: center;
  position: static;
  float: none;
  padding: 8rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddBtn.he-loadSuccessTextBox .he-reLoadText {
  font-size: 24rpx;
  color: #526085;
  padding-top: 4rpx;
  display: block;
  text-align: center;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddBtn {
  width: 152rpx;
  height: 152rpx;
  text-align: center;
  position: relative;
  float: left;
  padding: 8rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddBtn .he-videoFirstLoadBtn {
  display: block;
  height: 100%;
  background: url("https://m.allinmed.cn/image/img00/patientConsult/upload_photo_default@2x.png");
  background-size: 100% 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddBtn .he-videoInputBtn {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 1;
  opacity: 0;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddBtn.ev-videoLoadHide {
  display: none;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddFirstBtn {
  width: 152rpx;
  height: 152rpx;
  text-align: center;
  position: relative;
  float: left;
  padding: 8rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .he-videoAddFirstBtn .he-videoFirstLoadBtn {
  display: block;
  height: 100%;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/upload_photo_default@2x.png");
  background-size: 100% 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-upLoadVideoInput {
  display: none;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList {
  position: relative;
  width: 152rpx;
  height: 152rpx;
  float: left;
  margin: 8rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList ._img {
  position: relative;
  width: 100%;
  height: 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .tc-upLoadCover {
  position: absolute;
  display: inline-block;
  top: 0;
  left: 0;
  width: 152rpx;
  height: 152rpx;
  background-color: #000000;
  opacity: 0.63;
  z-index: 0;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .upload-fail {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0.83;
  background: #545454;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .upload-fail > ._input {
  opacity: 0;
  width: 100%;
  height: 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .upload-fail > ._p {
  font-size: 24rpx;
  color: #ffffff !important;
  text-align: center;
  position: absolute;
  top: 50%;
  width: 100%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .tc-upLoadDel {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 8rpx;
  right: 8rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/symptom_photo_delete@2x.png") no-repeat;
  background-position: top right;
  background-size: 38rpx 38rpx;
  z-index: 1;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .tc-upLoading {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 44rpx;
  left: 50%;
  margin-left: -20rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/symptom_photo_loading@2x.png") no-repeat center;
  background-size: 100% 100%;
  animation: submitIng 1s linear infinite;
  -webkit-animation: submitIng 1s linear infinite;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .tc-upLoadAfresh {
  color: #ffffff;
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 44rpx;
  left: 50%;
  margin-left: -20rpx;
  background: url("https://m.allinmed.cn/image/img00/patientConsult/symptom_refresh_loading@2x.png") no-repeat center;
  background-size: 100% 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .tc-upLoadAfreshText {
  font-size: 26rpx;
  display: inline-block;
  position: absolute;
  left: 50%;
  margin-left: -76rpx;
  bottom: 30rpx;
  color: #ffffff;
  width: 152rpx;
  text-align: center;
  z-index: 1;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageItemList .ev-fileUpRefresh {
  position: absolute;
  display: inline-block;
  top: 0;
  left: 0;
  width: 152rpx;
  height: 152rpx;
  margin-top: 8rpx;
  margin-left: 8rpx;
  z-index: 2;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageUpLoadAdd {
  width: 152rpx;
  height: 152rpx;
  text-align: center;
  position: relative;
  float: left;
  padding: 8rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-loadFiles .tc-imageUpLoadAdd .imageIcon {
  display: block;
  height: 100%;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/upload_photo_default@2x.png");
  background-size: 100% 100%;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-videosSubmit {
  text-align: center;
  position: absolute;
  bottom: 60rpx;
  padding: 0 90rpx;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-videosSubmit .usable {
  display: none;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-videosSubmit ._button.unusable {
  background-color: #dfdfdf;
}
.he-videoUpHide .he-videoUpLoadBox .he-videosMain .he-videosSubmit ._button {
  width: 570rpx;
  display: block;
  line-height: 70rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #00d6c6;
  color: white;
  border-radius: 45rpx;
}
.video-upLoad-box {
  position: fixed;
  z-index: 5;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0);
}
.ev-videoUpLoading {
  width: 258rpx;
  height: 258rpx;
  position: fixed;
  top: 50%;
  margin-top: -129rpx;
  margin-left: -129rpx;
  left: 50%;
  right: 0;
  opacity: 0.8;
  background: #000000;
  border-radius: 6rpx;
}
.ev-videoUpLoading .tc-videoLoadingImg {
  padding-top: 66rpx;
}
.ev-videoUpLoading .tc-videoLoadingImg ._img {
  margin: 0 auto;
  width: 60rpx;
  height: 60rpx;
  display: block;
  animation: rotate 1s linear forwards infinite;
}
.ev-videoUpLoading .tc-videoLoadingText {
  padding-top: 35rpx;
  font-size: 28rpx;
  color: #ffffff;
  text-align: center;
}
.popup-tips {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.5s;
}
.fade-enter,
.fade-leave-to {
  opacity: 0;
}
@keyframes submitIng {
0% {
    -webkit-transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
}
}