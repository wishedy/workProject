@import "/vendor/css/main.wxss";

.tc-upLoadFile.fileUpload {
  display: flex;
}
.tc-upLoadFile {
  background-color: #ffffff;
  padding-top: 22rpx;
  box-sizing: border-box;
}
.upload-wrapper {
  width: 100%;
  min-height: 100%;
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background-color: #ffffff;
}
.upload-wrapper .al-uploadNumLimit {
  font-size: 32rpx;
  padding: 30rpx 60rpx 30rpx 48rpx;
}
.upload-wrapper .al-uploadNumLimit ._span {
  display: inline-block;
  position: relative;
  padding: 10rpx 30rpx 10rpx 65rpx;
  background-color: #fa787a2b;
  border-radius: 50rpx;
  width: 100%;
  box-sizing: border-box;
  color: #444444;
}
.upload-wrapper .al-uploadNumLimit ._span::before {
  position: absolute;
  content: "";
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background: url("https://m.allinmed.cn/static/image/img00/doctorHome/upLoadTip.png") no-repeat center;
  background-size: 28rpx 28rpx;
  top: 50%;
  margin-top: -14rpx;
  left: 20rpx;
}
.tc-upLoadBox {
  background-color: white;
}
.tc-upLoadBox .middle-tip-box {
  opacity: 0.83;
  background: #545454;
  position: absolute;
}
.tc-upLoadBox .middle-tip-box .middle-tip-modal {
  position: absolute;
}
.tc-upLoadBox .tc-upLoadTitle {
  padding: 30rpx 48rpx 30rpx 48rpx;
  position: relative;
  border-bottom: 1rpx solid #ffffff;
}
.tc-upLoadBox .tc-upLoadTitle.uploadTips-box {
  padding-left: 86rpx;
  background: url("https://m.allinmed.cn/static/image/img00/consult_V1.2/doubt2@2x.png") no-repeat 48rpx center;
  background-size: 38rpx;
}
.tc-upLoadBox .tc-upLoadTitle.uploadTips-box .tc-upLoadTitleName {
  color: #2EA9FE;
}
.tc-upLoadBox .tc-upLoadTitle.uploadTips-box .tc-upLoadRightIcon {
  float: right;
  display: inline-block;
  width: 26rpx;
  margin-top: 10rpx;
  height: 26rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/arrow.png");
  background-size: 100% 100%;
}
.tc-upLoadBox .tc-upLoadTitle .tc-upLoadTitleName {
  display: inline-block;
  font-size: 32rpx;
  color: #222222;
}
.tc-upLoadBox .tc-upLoadTitle .ev-upLoadInput {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0;
  width: 100%;
}
.tc-upLoadBox .tc-upLoadTitle .tc-upLoadRightIcon {
  float: right;
  display: inline-block;
  width: 26rpx;
  margin-top: 10rpx;
  height: 26rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/file_upload@2x.png");
  background-size: 100% 100%;
}
.tc-upLoadBox .tc-upLoadTitle .tc-upLoadRightCover {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 5;
  display: none;
}
.tc-upLoadBox .tc-upLoadTitle .input-box {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.tc-upLoadBox .tc-upLoadTitle .input-box .inputBoxClass {
  height: 100%;
}
.tc-upLoadBox .tc-upLoadTitle .input-box .inputBoxClass .ev-upLoadInput {
  height: 100%;
  opacity: 0;
  border: none;
  width: 100%;
}
.tc-upLoadBox .tc-upLoadTitle .input-box.isWx .inputBoxClass {
  height: 100%;
}
.tc-upLoadBox .tc-upLoadTitle .input-box.isWx .inputBoxClass .ev-upLoadInput {
  height: 100%;
  opacity: 0;
  border: none;
  width: 100%;
}
.tc-upLoadBox .tc-upLoadItemBox {
  padding-left: 32rpx;
  padding-right: 30rpx;
  padding-bottom: 36rpx;
}
.tc-upLoadBox .tc-upLoadItemBox:after {
  content: '';
  display: table;
  clear: both;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList {
  position: relative;
  width: 152rpx;
  height: 152rpx;
  float: left;
  margin: 8rpx;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList ._img {
  position: relative;
  width: 100%;
  height: 100%;
  vertical-align: top;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadCover {
  position: absolute;
  display: inline-block;
  top: 0;
  left: 0;
  width: 152rpx;
  height: 152rpx;
  background-color: #000000;
  opacity: 0.63;
  z-index: 0;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .upload-fail {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0.83;
  background: #545454;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .upload-fail > ._input {
  opacity: 0;
  width: 100%;
  height: 100%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .upload-fail > ._p {
  font-size: 24rpx;
  color: #ffffff !important;
  text-align: center;
  position: absolute;
  top: 50%;
  width: 100%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadDel {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 8rpx;
  right: 8rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/symptom_photo_delete@2x.png") no-repeat;
  background-position: top right;
  background-size: 38rpx 38rpx;
  z-index: 1;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadAddMore {
  display: inline-block;
  width: 100%;
  height: 1000%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadAddMore:before {
  display: inline-block;
  content: "";
  position: absolute;
  width: 64rpx;
  height: 2rpx;
  background: #d8d8d8;
  top: 50%;
  left: 50%;
  margin-left: -32rpx;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadAddMore:after {
  display: inline-block;
  content: "";
  position: absolute;
  width: 2rpx;
  height: 64rpx;
  background: #d8d8d8;
  top: 50%;
  margin-top: -32rpx;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoading {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 44rpx;
  left: 50%;
  margin-left: -20rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/symptom_photo_loading@2x.png") no-repeat center;
  background-size: 100% 100%;
  animation: submitIng 1s linear infinite;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadAfresh {
  color: #ffffff;
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 44rpx;
  left: 50%;
  margin-left: -20rpx;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/symptom_refresh_loading@2x.png") no-repeat center;
  background-size: 100% 100%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .tc-upLoadAfreshText {
  font-size: 26rpx;
  display: inline-block;
  position: absolute;
  left: 50%;
  margin-left: -76rpx;
  bottom: 30rpx;
  color: #ffffff;
  width: 152rpx;
  text-align: center;
  z-index: 1;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .ev-fileUpRefresh {
  position: absolute;
  display: inline-block;
  top: 0;
  left: 0;
  width: 152rpx;
  height: 152rpx;
  margin-top: 8rpx;
  margin-left: 8rpx;
  z-index: 2;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadItemList .ev-upLoadInput {
  margin: 0;
  height: 100%;
  opacity: 0;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadAdd {
  width: 152rpx;
  height: 152rpx;
  text-align: center;
  position: relative;
  float: left;
  padding: 8rpx;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadAdd ._input {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0;
  width: 100%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadAdd .upLoadBtnBox {
  display: block;
  height: 100%;
  background: url("https://m.allinmed.cn/static/image/img00/patientConsult/upload_photo_default@2x.png");
  background-size: 100% 100%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadAdd .upLoadBtnBox .tc-upLoadAddMore {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadAdd .upLoadBtnBox .tc-upLoadAddMore ._section {
  height: 100%;
}
.tc-upLoadBox .tc-upLoadItemBox .tc-upLoadAdd .upLoadBtnBox .tc-upLoadAddMore ._section .ev-upLoadInput {
  display: block;
  height: 100%;
}
.tc-upLoadBox .tc-upLoadItemBox ._div {
  margin-top: 8rpx;
  margin-left: 8rpx;
}
.tc-upLoadBox .tc-upLoadItemBox.tc-reviewItemBox {
  display: block;
}
.tc-upLoadSubmit {
  padding-top: 70rpx;
  text-align: center;
  padding-bottom: 60rpx;
  background-color: white;
}
.tc-upLoadSubmit .tc-submitBtn {
  width: 570rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #00d6c6;
  color: white;
  border-radius: 45rpx;
  margin: 0 auto;
}
.tc-upLoadSubmit .tc-submitDisabled {
  width: 570rpx;
  display: block;
  line-height: 70rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #dfdfdf;
  color: white;
  border-radius: 45rpx;
  margin: 0 auto;
}
.he-upLoadGuide ._img {
  max-width: 100%;
  min-width: 100%;
  height: 100%;
  width: 100%;
}
#EV-swiper {
  padding-top: 40rpx;
}
#EV-swiper .closeBtn {
  top: 40rpx;
  right: 40rpx;
}
#EV-swiper .EV-gallery-top {
  font-size: 28rpx;
}
#EV-swiper .swiper-pagination.swiper-pagination-bullets {
  height: 37rpx;
}
#EV-swiper .swiper-pagination.swiper-pagination-bullets .swiper-pagination-bullet-active,
#EV-swiper .swiper-pagination.swiper-pagination-bullets .swiper-pagination-bullet {
  font-size: 28rpx;
  width: 16rpx;
  height: 16rpx;
}
#EV-swiper .swiper-wrapper {
  height: 100%;
}
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.5s;
}
.fade-enter,
.fade-leave-to {
  opacity: 0;
}