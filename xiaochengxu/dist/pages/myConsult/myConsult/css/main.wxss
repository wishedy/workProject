@import "/vendor/css/main.wxss";

.left {
  float: left;
}
.right {
  float: right;
}
.orderList {
  background-color: #ffffff;
}
.noFriendText {
  padding-top: 470rpx;
}
.noFriendText:before {
  content: '';
  display: block;
  width: 328rpx;
  height: 328rpx;
  background: url("https://m1.allinmed.cn/static/image/img00/myServices/inquiry_bg.png") no-repeat center center;
  background-size: 100% 100%;
  position: absolute;
  top: 94rpx;
  left: 50%;
  margin-left: -164rpx;
}
.noFriendText ._p {
  height: 30rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #999999;
  line-height: 30rpx;
  text-align: center;
}
.noFriendHref ._div {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  line-height: 96rpx;
  margin: 0 auto;
  text-align: center;
  margin-top: 176rpx;
  color: #FFFFFF;
  font-weight: bold;
  font-size: 36rpx;
  color: white;
  width: 520rpx;
  height: 96rpx;
  background: #2ea9fe;
  border-radius: 8rpx;
}
.noFriendHref ._div .icon {
  display: block;
  width: 44rpx;
  height: 44rpx;
  background: url("https://m1.allinmed.cn/static/image/img00/myServices/advisory.png") no-repeat center center/cover;
  margin-right: 6rpx;
}
.orderList {
  box-sizing: border-box;
  height: 100%;
}
.orderList .orderHistoryBox {
  background: #eeeeee;
  border-top: 1rpx solid transparent;
  min-height: 100%;
}
.orderList .orderHistoryBox .noMore {
  line-height: 100rpx;
  color: #666;
  text-align: center;
  font-size: 28rpx;
}
.orderList .orderHistoryItem {
  margin: 20rpx 20rpx 0;
  background: #ffffff;
  border-radius: 16rpx;
}
.orderList .orderHistoryItem .orderHisItemTop {
  padding: 32rpx 32rpx 24rpx 32rpx;
}
.orderList .orderHistoryItem .orderHisItemTop:after {
  content: '';
  display: table;
  clear: both;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo:after {
  content: '';
  display: table;
  clear: both;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo .docLogo > ._img {
  display: block;
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo .docType {
  padding-left: 16rpx;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo .docType .docName {
  color: #222222;
  font-weight: bold;
  font-size: 30rpx;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo .docType .docName .medicalTitleRight {
  margin-left: 16rpx;
  font-weight: normal;
  color: #333333;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo .docType .inquiryType {
  color: #909090;
  font-size: 24rpx;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorInfo .docType .inquiryType.mTop {
  margin-top: 10rpx;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorState {
  text-align: right;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorState .inquiryTimeShow {
  display: block;
  font-size: 24rpx;
  color: #AAAAAA;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorState .inquiryStateShow {
  display: block;
  font-size: 30rpx;
  font-weight: bold;
  color: #00B9AD;
}
.orderList .orderHistoryItem .orderHisItemTop .doctorState .inquiryStateShow.font-gray {
  color: #AAAAAA;
}
.orderList .orderHistoryItem .orderHistoryItemCenter {
  position: relative;
  padding: 0 32rpx 44rpx;
  font-size: 32rpx;
}
.orderList .orderHistoryItem .orderHistoryItemCenter:before {
  display: block;
  content: "";
  height: 1rpx;
  background: #F8F8F8;
  margin-bottom: 32rpx;
  margin-right: 40rpx;
}
.orderList .orderHistoryItem .orderHistoryItemCenter ._p {
  color: #909090;
  line-height: 32rpx;
}
.orderList .orderHistoryItem .orderHistoryItemCenter ._p:nth-child(2) {
  padding-top: 20rpx;
}
.orderList .orderHistoryItem .orderHistoryItemCenter ._p ._i {
  display: inline-block;
  font-style: normal;
  vertical-align: middle;
}
.orderList .orderHistoryItem .orderHistoryItemCenter ._p ._span {
  display: inline-block;
  color: #555555;
  margin-left: 20rpx;
  vertical-align: middle;
}
.orderList .orderHistoryItem .orderHistoryItemCenter .patientComplaint {
  width: 480rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.orderList .orderHistoryItem .orderHistoryItemBottom {
  margin: 0 60rpx 0 32rpx;
  border-top: 2rpx solid #F2F2F2;
  display: flex;
  display: -webkit-flex;
  -webkit-justify-content: space-around;
}
.orderList .orderHistoryItem .orderHistoryItemBottom .hrefBtn {
  margin: 20rpx 0 28rpx;
  padding: 10rpx 28rpx;
  background: #fff;
  color: #00B9AD;
  font-size: 28rpx;
  border: 1rpx solid rgba(47, 197, 189, 0.5);
  border-radius: 50rpx;
}
.orderList .orderMessageItem {
  width: 710rpx;
  height: 288rpx;
  background: #fefefe;
  box-shadow: 0 6rpx 18rpx 0 rgba(131, 149, 162, 0.15);
  border-radius: 16rpx;
  margin: 20rpx 20rpx 0;
}
.orderList .orderMessageItem .orderMessageDoctor {
  width: 656rpx;
  height: 130rpx;
  background: #e7f2fd;
  border-radius: 16rpx 16rpx 0 0;
  padding-top: 30rpx;
  padding-left: 28rpx;
  padding-right: 26rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  position: relative;
}
.orderList .orderMessageItem .orderMessageDoctor .doctorLogo {
  width: 100rpx;
  height: 100rpx;
  background: #b0d4fb;
  opacity: 0.85;
  border-radius: 50%;
  margin-right: 22rpx;
  border: 4rpx solid white;
}
.orderList .orderMessageItem .orderMessageDoctor .doctorInfo {
  display: block;
}
.orderList .orderMessageItem .orderMessageDoctor .doctorInfo .doctorMedicalTitle {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  margin-bottom: 18rpx;
  height: 50rpx;
  justify-content: flex-start;
}
.orderList .orderMessageItem .orderMessageDoctor .doctorInfo .doctorMedicalTitle .doctorName {
  height: 34rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #08112b;
  line-height: 34rpx;
}
.orderList .orderMessageItem .orderMessageDoctor .doctorInfo .doctorMedicalTitle .medicalTitle {
  height: 32rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #08112b;
  line-height: 32rpx;
  margin-left: 10rpx;
}
.orderList .orderMessageItem .orderMessageDoctor .doctorInfo .doctorCompany {
  display: block;
  width: 308rpx;
  height: 26rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #08112b;
  line-height: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.orderList .orderMessageItem .orderMessageDoctor .messageTime {
  height: 26rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #0d162f;
  line-height: 26rpx;
  position: absolute;
  right: 26rpx;
  top: 48rpx;
}
.orderList .orderMessageItem .orderMessageDoctor .messageNum {
  width: 142rpx;
  height: 48rpx;
  padding-right: 26rpx;
  background: #f27587;
  box-shadow: 0 4rpx 6rpx 0 rgba(242, 117, 135, 0.53);
  border-radius: 8rpx 0 0 8rpx;
  position: absolute;
  right: 0;
  bottom: 24rpx;
  font-size: 24rpx;
  text-align: right;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: white;
  line-height: 48rpx;
}
.orderList .orderMessageItem .orderMessagePatient {
  padding-top: 26rpx;
  padding-left: 40rpx;
}
.orderList .orderMessageItem .orderMessagePatient .labelName {
  width: 86rpx;
  height: 26rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #666666;
  line-height: 26rpx;
  margin-right: 22rpx;
  text-align-last: justify;
  text-align: justify;
}
.orderList .orderMessageItem .orderMessagePatient .patientItem {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  height: 26rpx;
  width: auto;
}
.orderList .orderMessageItem .orderMessagePatient .patientItem:nth-of-type(2) {
  margin-top: 14rpx;
}
.orderList .orderMessageItem .orderMessagePatient .patientItem .patientDetailInfo {
  height: 26rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #909090;
  line-height: 26rpx;
}
.orderList .orderMessageItem .orderMessagePatient .patientItem.patientDetail .patientDetailInfo {
  width: 416rpx;
  height: 26rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #909090;
  line-height: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.authorization {
  margin-top: 180rpx;
  background: #fff;
}
.authorization image {
  width: 360rpx;
  height: 256rpx;
  display: block;
  margin: 0 auto 28rpx;
}
.authorization .text {
  font-size: 28rpx;
  color: #888888;
  letter-spacing: 0;
  line-height: 28rpx;
  text-align: center;
  margin-bottom: 140rpx;
}
.authorization ._button {
  width: 400rpx;
  height: 100rpx;
  border: 2rpx solid #12D5A8;
  border-radius: 50rpx;
  line-height: 100rpx;
  color: #11D5A7;
  display: block;
  margin: 0 auto;
}