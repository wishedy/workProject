@import "/vendor/css/main.wxss";

@charset "UTF-8";
page,
page {
  width: 100%;
  height: 100%;
  background-color: #f2f2f2;
}
.question-feedback {
  color: #aaaaaa;
  letter-spacing: 0;
  font-size: 30rpx;
  position: relative;
  background: #f3f7f7;
  padding: 34rpx 0 22rpx 30rpx;
}
.main-inner {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background: #f3f7f7;
}
.main-inner-content {
  box-sizing: border-box;
  z-index: 2;
}
.feedback-header {
  padding-top: 21rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.feedback-header-top {
  font-size: 44rpx;
  color: #555555;
  font-weight: normal;
}
.feedback-header-bottom {
  margin-top: 10rpx;
  margin-bottom: 36rpx;
  font-size: 30rpx;
  color: #909090;
}
.feedback-main {
  padding-bottom: 60rpx;
}
.feedback-main .feedback.form {
  background-color: white;
}
.feedback-main .question {
  box-sizing: border-box;
  margin-left: 0;
}
.feedback-main .check-box {
  margin-top: 26rpx;
  margin-left: 14rpx;
  width: 20rpx;
  height: 20rpx;
  border: 1rpx solid #cccccc;
  border-radius: 2rpx;
}
.question-type {
  padding-top: 27rpx;
  padding-left: 80rpx;
  font-size: 36rpx;
  color: #333333;
  background: #ffffff;
  position: relative;
  padding-right: 48rpx;
}
.question-type.on:before {
  background: url("https://m.allinmed.cn/static/image/img00/consult_V1.2/multiplechoice_on@2x.png");
  background-size: contain;
}
.question-type:before {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  background: url("https://m.allinmed.cn/static/image/img00/consult_V1.2/multiplechoice_off@2x.png");
  background-size: contain;
  border-radius: 2rpx;
  content: "";
  position: absolute;
  left: 30rpx;
  top: 34rpx;
}
.question-type ._span {
  width: 100%;
  display: inline-block;
  vertical-align: middle;
  border-bottom: 1rpx solid #f2f2f2;
  padding-bottom: 22rpx;
  margin-right: 48rpx;
}
.question-description {
  padding-top: 14rpx;
  padding-left: 60rpx;
  font-size: 30rpx;
  color: #666666;
  background: #ffffff;
}
.check-box {
  margin-top: 26rpx;
  margin-left: 14rpx;
  width: 20rpx;
  height: 20rpx;
  border: 1rpx solid #cccccc;
  border-radius: 2rpx;
}
.main-input-box-textarea-inner {
  box-sizing: border-box;
  overflow: auto;
  background: #ffffff;
  position: relative;
}
.main-input-box-textarea-inner .input-textArea {
  width: 100%;
  padding: 26rpx 30rpx 10rpx;
  font-size: 28rpx;
  border: 1rpx solid #e8ecef;
  height: 248rpx;
  border-style: none;
  box-sizing: border-box;
}
.input-textArea2 {
  width: 100%;
  background: #ffffff;
  padding-top: 10rpx;
  padding-left: 30rpx;
  border: none;
  font-size: 28rpx;
  color: #444444;
  height: 104rpx;
  word-break: break-all;
}
.go-next {
  width: 560rpx;
  height: 100rpx;
  line-height: 100rpx;
  display: block;
  border-radius: 8rpx;
  background: #cccccc;
  text-align: center;
  box-sizing: border-box;
  font-size: 36rpx;
  color: #fff;
  outline: medium;
  padding: 0;
  margin: 80rpx auto 40rpx;
}
.go-next.active {
  box-shadow: 0 8rpx 24rpx 0 rgba(47, 197, 189, 0.4);
  background: #2EA9FE;
}
.text-num-tips {
  position: absolute;
  font-size: 26rpx;
  color: #909090;
  bottom: 18rpx;
  right: 30rpx;
}
.get-back {
  top: 50%;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  text-align: center;
}
.get-back .icon {
  background: url("https://m.allinmed.cn/static/image/img00/directOperation/select_yes.png") no-repeat bottom center;
  background-size: contain;
  text-align: center;
  margin-top: 208rpx;
  width: 112rpx;
  height: 112rpx;
  display: inline-block;
}
.get-back .title {
  margin-top: 14rpx;
  margin-bottom: 30rpx;
  text-align: center;
  font-size: 48rpx;
  color: #555555;
  box-sizing: border-box;
}
.get-back .description {
  margin-top: 30rpx;
  margin-bottom: 84rpx;
  text-align: center;
  font-size: 36rpx;
  color: #999999;
}
.get-back .back {
  text-align: center;
  margin-bottom: 234rpx;
}
.get-back .back > ._em {
  font-style: normal;
  color: #2EA9FE;
}
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.5s;
}
.fade-enter,
.fade-leave-to {
  opacity: 0;
}
.back .back-timing {
  display: inline-block;
}
.back ._em {
  display: inline-block;
}
.contactMain {
  height: 100%;
  position: relative;
  background: #F3F7F7;
  background-size: 100% 272rpx;
}
.contactMain .con_center {
  background: #ffffff;
  padding-bottom: 45rpx;
}
.contactMain .con_center .con_itemList .con_workTimeBox {
  padding-top: 42rpx;
  padding-bottom: 26rpx;
  padding-left: 60rpx;
  background: #F3F7F7;
}
.contactMain .con_center .con_itemList .con_workTimeBox.notWork {
  margin-top: 50rpx;
}
.contactMain .con_center .con_itemList .con_workTimeBox .workingTitle {
  font-size: 36rpx;
  color: #333333;
  font-weight: bold;
  position: relative;
}
.contactMain .con_center .con_itemList .con_workTimeBox .workingTitle .con_point {
  position: absolute;
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  -webkit-border-radius: 100rpx;
  -moz-border-radius: 100rpx;
  border-radius: 100rpx;
  background-color: #2EA9FE;
  top: 50%;
  margin-top: -8rpx;
  left: -32rpx;
}
.contactMain .con_center .con_itemList .con_workTimeBox .workingWeek {
  font-size: 28rpx;
  color: #909090;
  padding-left: 16rpx;
}
.contactMain .con_center .con_itemList .con_workTimeBox .workingHour {
  font-size: 28rpx;
  color: #909090;
  padding: 0 10rpx;
}
.contactMain .con_center .con_itemList .con_workTimeBox .workingTip {
  font-size: 28rpx;
  color: #909090;
}
.contactMain .con_center .con_itemList .con_phone {
  padding-left: 60rpx;
  padding-top: 44rpx;
}
.contactMain .con_center .con_itemList .con_phone ._a {
  display: inline-block;
}
.contactMain .con_center .con_itemList .con_phone .con_wxText {
  padding-right: 32rpx;
}
.contactMain .con_center .con_itemList .con_phone .con_phoneText {
  font-size: 36rpx;
  color: #909090;
  padding-right: 32rpx;
}
.contactMain .con_center .con_itemList .con_phone .con_phoneNum {
  font-size: 36rpx;
  color: #2EA9FE;
  display: inline-block;
  line-height: 0.9;
  font-weight: bold;
  border-bottom: 4rpx solid #2EA9FE;
}
.contactMain .con_center .con_itemList .con_phone .con_phoneIcon {
  margin-left: 30rpx;
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  vertical-align: bottom;
  background: url("https://m.allinmed.cn/static/image/mp/index/1.4.0/phone.png") no-repeat center;
  background-size: 100% 100%;
}
.contactMain .con_center .con_itemList .con_weiXin {
  padding-left: 60rpx;
  padding-top: 36rpx;
}
.contactMain .con_center .con_itemList .con_weiXin .con_wxText {
  font-size: 36rpx;
  color: #909090;
  padding-right: 32rpx;
}
.contactMain .con_center .con_itemList .con_weiXin .con_wxNum {
  font-size: 36rpx;
  color: #222222;
}
.contactMain .con_center .con_itemList .con_email {
  padding-left: 60rpx;
  padding-top: 45rpx;
}
.contactMain .con_center .con_itemList .con_email .con_wxText {
  font-size: 36rpx;
  color: #909090;
  padding-right: 32rpx;
}
.contactMain .con_center .con_itemList .con_email .con_wxNum {
  font-size: 36rpx;
  color: #222222;
}