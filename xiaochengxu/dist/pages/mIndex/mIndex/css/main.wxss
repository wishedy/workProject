@import "/vendor/css/main.wxss";

.indexBox {
  width: 100%;
  margin: 32rpx 0 56rpx;
}
.indexBox:after {
  content: '';
  display: table;
  clear: both;
}
.indexBox .halfPart1 {
  float: left;
  width: 334rpx;
  height: 300rpx;
  padding: 0;
  margin-left: 30rpx;
  background: linear-gradient(315deg, #c0e9ff 0%, #71a6f5 100%);
  box-shadow: 0 10rpx 20rpx 0 rgba(145, 185, 248, 0.3);
  border-radius: 8rpx;
}
.indexBox .halfPart1 ._img {
  display: block;
  width: 100%;
  height: 100%;
}
.indexBox .halfPart2 {
  float: right;
  width: 334rpx;
  height: 300rpx;
  padding: 0;
  margin-right: 30rpx;
  background: linear-gradient(314deg, #fdd5bb 0%, #f47689 100%);
  box-shadow: 0 10rpx 20rpx 0 rgba(193, 116, 127, 0.3);
  border-radius: 8rpx;
}
.indexBox .halfPart2 ._img {
  display: block;
  width: 100%;
  height: 100%;
}
.registered {
  width: 690rpx;
  height: 200rpx;
  margin: 32rpx 30rpx;
  margin-bottom: 0;
  padding: 0;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  border: 2rpx solid #f6f6f7;
  box-shadow: 0 6rpx 20rpx rgba(7, 113, 170, 0.11);
}
.registered ._img {
  width: 690rpx;
  height: 200rpx;
}
.quickSearch {
  margin: 0 30rpx 50rpx;
}
.quickSearch ._h1 {
  font-size: 30rpx;
  color: #777777;
  letter-spacing: 0;
  line-height: 30rpx;
  margin-bottom: 40rpx;
}
.quickSearch ._h1:before {
  display: inline-block;
  content: "";
  width: 4rpx;
  height: 26rpx;
  margin-right: 10rpx;
  background: #02B5AC;
  border-radius: 100rpx;
  vertical-align: -2rpx;
}
.quickSearch .quickSearchItem {
  font-size: 28rpx;
  color: #02B5AC;
  line-height: 56rpx;
  border: 1rpx solid rgba(2, 181, 172, 0.6);
  display: inline-block;
  background: #ffffff;
  border-radius: 4rpx;
  margin-right: 24rpx;
  margin-bottom: 18rpx;
  padding: 0 20rpx;
}
.mHome {
  background-color: #ffffff;
}
.searchDocOut {
  padding: 10rpx 30rpx;
  margin-top: -2rpx;
}
.searchDoc {
  height: 76rpx;
  line-height: 76rpx;
  background: #F6F6F6;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #999999;
  box-sizing: border-box;
}
.searchDoc:before {
  width: 38rpx;
  height: 38rpx;
  display: inline-block;
  margin-left: 20rpx;
  content: "";
  margin-right: 8rpx;
  margin-top: -5rpx;
  background: url("https://m1.allinmed.cn/static/image/mp/index/1.5.0/index_search.png") no-repeat;
  background-size: contain;
  vertical-align: middle;
}
.input-box .search {
  padding-left: 50rpx;
  font-size: 30rpx;
  width: 650rpx;
  outline: none;
  border: none;
  border-radius: 100rpx;
  color: black;
  height: 70rpx;
  z-index: 5;
  background-color: #F6F6F6;
}
.input-box .search::-webkit-input-placeholder {
  color: #7A8585;
  text-align: center;
}
.input-box .search::-moz-placeholder {
  color: #7A8585;
  text-align: center;
}
.input-box .search:-moz-placeholder {
  color: #7A8585;
  text-align: center;
}
.input-box .search:-ms-input-placeholder {
  color: #7A8585;
  text-align: center;
}
.AIminiProgram {
  position: relative;
  height: 140rpx;
  margin: 0 30rpx 30rpx 30rpx;
  border-radius: 6rpx;
  overflow: hidden;
}
.AIminiProgram ._img {
  width: 100%;
  height: 100%;
}
.AIminiProgram .AIminiProgram-item {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.health-card {
  width: 100%;
  height: 128rpx;
  background: url("http://m.allinmed.cn/static/image/mp/index/1.4.0/banner.png") no-repeat center;
  background-size: 100% 100%;
  margin-bottom: 26rpx;
}
.middle-box {
  height: 16rpx;
  background: #f2f2f2;
}
.history {
  padding-bottom: 150rpx;
}
.history ._header {
  padding: 44rpx 30rpx 32rpx;
}
.history ._header:after {
  content: '';
  display: table;
  clear: both;
}
.history ._header .journalTitle {
  display: inline-block;
  font-size: 30rpx;
  color: #777777;
  letter-spacing: 0;
  line-height: 30rpx;
}
.history ._header .journalTitle:before {
  display: inline-block;
  content: "";
  width: 4rpx;
  height: 26rpx;
  margin-right: 10rpx;
  background: #2EA9FE;
  border-radius: 100rpx;
  vertical-align: -2rpx;
}
.history ._header .addJournal {
  float: right;
  text-align: left;
  height: 56rpx;
  display: flex;
  align-content: center;
  padding: 0;
  margin: 0;
  background-color: rgba(186, 186, 186, 0.1);
  border-radius: 100rpx 0 0 100rpx;
}
.history ._header .addJournal .addIcon {
  width: 56rpx;
  height: 56rpx;
}
.history ._header .addJournal .addText {
  font-size: 28rpx;
  line-height: 56rpx;
  margin: 0 12rpx;
  color: #2EA9FE;
}
.history .journalClassifyList {
  line-height: 88rpx;
  height: 88rpx;
  white-space: nowrap;
  overflow-x: scroll;
  overflow-y: hidden;
  border-bottom: 2rpx solid #EBEDF0;
}
.history .journalClassifyList .journalClassifyItem {
  display: inline-block;
  padding: 5rpx 25rpx;
  font-size: 30rpx;
  color: #555555;
}
.history .journalClassifyList .journalClassifyItem.selected {
  font-size: 38rpx;
  font-weight: bold;
  color: #333333;
  position: relative;
}
.history .journalClassifyList .journalClassifyItem.selected:after {
  position: absolute;
  display: block;
  content: '';
  width: 32rpx;
  height: 6rpx;
  background: #2EA9FE;
  bottom: 8rpx;
  left: 50%;
  margin-left: -16rpx;
  border-radius: 99rpx;
}
.history .extraLine {
  display: block;
  text-align: center;
  font-size: 26rpx;
  color: #BDBDBD;
  margin-top: 50rpx;
}