@import "/vendor/css/main.wxss";

.out-box {
  min-height: 100%;
  background-color: #fff;
}
.image-head {
  height: 400rpx;
  background: url("https://m.allinmed.cn/static/image/img00/login/banner@2x.png") center no-repeat;
  background-size: 100% 100%;
}
.loginRegisterBox {
  padding: 20rpx 75rpx 0;
  font-size: 40rpx;
  color: #aaaaaa;
  font-weight: 600;
  box-sizing: border-box;
}
.loginRegisterBox:after {
  content: '';
  display: table;
  clear: both;
}
.loginRegisterBox .tphoneTips {
  margin-top: 60rpx;
  font-size: 30rpx;
  color: #909090;
  letter-spacing: 0;
  font-weight: normal;
}
.loginRegisterBox ._p {
  width: 100%;
  border: 1rpx solid #898989;
  box-sizing: border-box;
  padding: 36rpx 30rpx;
  border-radius: 10rpx;
  font-size: 34rpx;
}
.loginRegisterBox ._p ._input {
  outline: none;
  border: none;
  color: #101010;
  height: 100%;
  font-weight: 600;
}
.loginRegisterBox ._p ._input.hasContent {
  font-size: 40rpx;
}
.loginRegisterBox ._p ._input .placeholderClass {
  color: #a0a0a0;
  font-weight: normal;
}
.loginRegisterBox ._p.phoneInput {
  margin-top: 36rpx;
  position: relative;
}
.loginRegisterBox ._p.phoneInput:after {
  content: '';
  display: table;
  clear: both;
}
.loginRegisterBox ._p.phoneInput > ._input {
  width: 100%;
  float: left;
}
.loginRegisterBox ._p.phoneInput > ._input.hasContent {
  width: 440rpx;
}
.loginRegisterBox ._p.phoneInput .icon-clear {
  display: block;
  position: absolute;
  width: 54rpx;
  height: 54rpx;
  top: 50%;
  margin-top: -27rpx;
  right: 30rpx;
  background: url("https://m.allinmed.cn/static/image/img00/login/close_button.png") center center no-repeat;
  background-size: 38rpx 38rpx;
}
.loginRegisterBox ._p.codeInput {
  margin-top: 40rpx;
  position: relative;
}
.loginRegisterBox ._p.codeInput:after {
  content: '';
  display: table;
  clear: both;
}
.loginRegisterBox ._p.codeInput > ._input {
  width: 220rpx;
  float: left;
}
.loginRegisterBox ._p.codeInput .icon-clear {
  display: inline-block;
  position: absolute;
  width: 54rpx;
  height: 54rpx;
  top: 50%;
  margin-top: -27rpx;
  right: 280rpx;
  background: url("https://m.allinmed.cn/static/image/img00/login/close_button.png") center center no-repeat;
  background-size: 38rpx 38rpx;
}
.loginRegisterBox ._p.codeInput.password .icon-clear {
  right: 83rpx;
}
.loginRegisterBox ._p.codeInput ._span {
  width: 38%;
  float: right;
  text-align: right;
}
.loginRegisterBox ._p.codeInput .getCode {
  font-weight: normal;
  color: #29a3a3;
  margin-top: 4rpx;
}
.loginRegisterBox ._p.codeInput .getCode.hasContent {
  margin-top: 16rpx;
}
.loginRegisterBox ._p.codeInput .codeCountdown {
  width: 50%;
  color: #777777;
  margin-top: 4rpx;
}
.loginRegisterBox ._p.codeInput .codeCountdown ._i {
  display: inline-block;
}
.loginRegisterBox .stipulation {
  display: block;
  color: #a0a0a0;
  font-size: 24rpx;
  margin-top: 20rpx;
  font-weight: normal;
}
.loginRegisterBox .stipulation ._i {
  display: inline-block;
  color: #29a3a3;
}
.loginRegisterBox .changeAndForget {
  padding: 20rpx 30rpx 0;
  font-size: 30rpx;
  color: #aaaaaa;
}
.loginRegisterBox .changeAndForget:after {
  content: '';
  display: table;
  clear: both;
}
.loginRegisterBox .changeAndForget .forgetPass {
  color: #2fc5bd;
}
.icon-eyesStatus {
  display: inline-block;
  width: 110rpx;
  height: 35rpx;
  background: url("https://m.allinmed.cn/static/image/img00/login/eyes_open.png") no-repeat;
  background-size: 100% 100%;
  margin-top: 5rpx;
}
.icon-eyesStatus.hasContent {
  margin-top: 20rpx;
}
.icon-eyesStatus.hide {
  background: url("https://m.allinmed.cn/static/image/img00/login/eyes_close.png") no-repeat;
  background-size: contain;
}
.icon-clearPassword {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
  background: url("https://m.allinmed.cn/static/image/img00/login/close_button.png") no-repeat;
  background-size: 100% 100%;
  margin-top: 5rpx;
}
.loginButton {
  display: block;
  width: 630rpx;
  line-height: 98rpx;
  color: #fff;
  background-image: linear-gradient(90deg, #07b6ac 6%, #3fe6bc 95%);
  box-shadow: 0 20rpx 36rpx 0 rgba(30, 231, 187, 0.38);
  border-radius: 200rpx;
  margin: 36rpx auto;
  box-sizing: border-box;
  text-align: center;
  font-size: 34rpx;
}
.popup-tips {
  position: absolute;
}