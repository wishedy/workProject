@import "/vendor/css/main.wxss";

.spread-box {
  padding-top: 66rpx;
}
.spread-main {
  padding: 0 60rpx;
}
.spread-main .doctor-info-box {
  display: flex;
  align-items: center;
}
.spread-main .doctor-info-box .doctor-icon {
  width: 96rpx;
  height: 96rpx;
  border-radius: 999rpx;
}
.spread-main .doctor-info-box .doctor-info {
  margin-left: 24rpx;
  line-height: 1;
  font-size: 30rpx;
  color: #9da3af;
}
.spread-main .doctor-info-box .doctor-info .doctor-name {
  font-size: 34rpx;
  font-weight: 500;
  color: #25324d;
}
.spread-main .doctor-info-box .doctor-info .doctor-title {
  margin-left: 18rpx;
}
.spread-main .doctor-info-box .doctor-info .doctor-info-hospital {
  margin-top: 18rpx;
}
.spread-main .spread-tips {
  width: 480rpx;
  height: 114rpx;
  box-sizing: border-box;
  margin-top: 8rpx;
  padding-top: 21rpx;
  line-height: 95rpx;
  background: url("https://m.allinmed.cn/static/image/mp/index/1.1.4/bubbles.png") no-repeat center;
  background-size: 100% 100%;
  font-size: 30rpx;
  font-weight: 500;
  color: white;
  text-align: center;
}
.spread-main .login-box {
  margin-top: 78rpx;
}
.spread-main .login-box .add-patient-content {
  padding: 15rpx 60rpx 0;
}
.spread-main .login-box .add-patient-content-item {
  height: 140rpx;
  line-height: 140rpx;
  border-bottom: 1rpx solid #D8D8D8;
  font-size: 0;
  position: relative;
}
.spread-main .login-box .add-patient-content-item > ._aside {
  display: inline-block;
  vertical-align: middle;
  width: 196rpx;
  font-size: 34rpx;
  color: #909090;
  letter-spacing: 0;
}
.spread-main .login-box .add-patient-content-item .codeText {
  position: absolute;
  right: 0;
  top: 0;
  font-size: 34rpx;
  color: #2FC5BD;
  letter-spacing: 0;
}
.spread-main .login-box .add-patient-content-item .codeText::after {
  content: "";
  height: 30rpx;
  width: 4rpx;
  background: #ccc;
  position: absolute;
  left: -14rpx;
  top: 50%;
  margin-top: -15rpx;
}
.spread-main .login-box .add-patient-content-item .codeCountdown {
  position: absolute;
  right: 0;
  top: 0;
  font-size: 34rpx;
  color: #777;
  letter-spacing: 0;
}
.spread-main .login-box .add-patient-content-item .codeCountdown ._i {
  display: inline-block;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  font-size: 34rpx;
  max-width: 380rpx;
  color: #666666;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input.birth-box {
  width: 50%;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input.code-box {
  width: 50%;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input ._input[disabled] {
  color: #555;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input .icon-clear {
  display: inline-block;
  position: absolute;
  width: 54rpx;
  height: 54rpx;
  top: 50%;
  margin-top: -27rpx;
  right: 150rpx;
  z-index: 2;
  background: url("https://m.allinmed.cn/static/image/img00/login/close_button.png") center center no-repeat;
  background-size: 38rpx 38rpx;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input .contentMessage {
  color: #AAAAAA;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input .contentMessage.on {
  color: #222222;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input ::-webkit-input-placeholder {
  color: #AFAFAF;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input :-moz-placeholder {
  color: #AFAFAF;
  opacity: 1;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input ::-moz-placeholder {
  color: #AFAFAF;
  opacity: 1;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input :-ms-input-placeholder {
  color: #AFAFAF;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input > ._input {
  width: 100%;
  height: 100%;
  border: none;
  outline: none;
  background: none;
  color: #222;
  font-size: 34rpx;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input > ._select {
  min-height: 60rpx;
  background: none;
  border: none;
  width: 100%;
  outline: medium;
}
.spread-main .login-box .add-patient-content-item > .add-patient-input > .codeInput {
  width: 60%;
}
.spread-main .login-box .add-patient-content-item .add-patient-sex-selector {
  display: inline-block;
  width: 164rpx;
  vertical-align: middle;
}
.spread-main .login-box .add-patient-content-item .add-patient-sex-selector .add-patient-selector {
  width: 48rpx;
  height: 48rpx;
  background: url("https://m.allinmed.cn/static//image/mp/consult/notSelect@2x.png") no-repeat;
  background-size: contain;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
  box-sizing: border-box;
}
.spread-main .login-box .add-patient-content-item .add-patient-sex-selector > ._span {
  display: inline-block;
  vertical-align: middle;
  color: #222222;
}
.spread-main .login-box .add-patient-content-item .add-patient-sex-selector.on .add-patient-selector {
  background: url("https://m.allinmed.cn/static//image/mp/consult/selected@2x.png") no-repeat;
  background-size: contain;
}
.spread-main .login-tips {
  margin-top: 28rpx;
  font-size: 34rpx;
  font-weight: 400;
  color: #fc746a;
}
.next-box {
  margin-top: 236rpx;
}
.next-box .next-btn {
  width: 520rpx;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  background: #00B9AD;
  color: #ffffff;
  border-radius: 8rpx;
  margin: 0 auto;
}
.next-box .next-tips {
  font-size: 28rpx;
  font-weight: 400;
  color: #aaaaaa;
  margin-top: 48rpx;
  text-align: center;
}
.next-box .next-tips .next-agreement {
  color: #00b9ad;
}
.journal-maskers {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.75);
  z-index: 2;
  transition: all 0.3s linear;
}
.journal-confirmBox-inner {
  width: 540rpx;
  background-color: #fff;
  position: absolute;
  border-radius: 24rpx;
  overflow: hidden;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.journal-confirmBox-content {
  padding: 42rpx 72rpx;
  font-size: 36rpx;
  border-bottom: 1rpx solid #dfdfdf;
  color: #222;
}
.journal-confirmBox-content ._p {
  font-weight: bold;
  text-align: center;
  margin-top: 5rpx;
}
.journal-confirmBox-btns .journal-confirmBox-ensureBtn {
  width: 100%;
  font-size: 34rpx;
  text-align: center;
  color: #00BEAF;
  font-weight: bold;
  background: #fff;
}